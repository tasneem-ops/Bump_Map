<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MapsDemo" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Blue_700</item>
        <item name="colorPrimaryVariant">@color/Blue_700</item>
        <item name="colorOnPrimary">@color/Blue_900</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/Blue_200</item>
        <item name="colorSecondaryVariant">@color/Blue_400</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <item name="fontFamily">@font/allan</item>
        <item name="android:backgroundTint">@color/Blue_50</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/Blue_900</item>
        <!-- Customize your theme here. -->
    </style>
</resources>